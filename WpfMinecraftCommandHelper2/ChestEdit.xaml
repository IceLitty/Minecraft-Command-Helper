        <TextBox x:Name="ContainerLTSeed" HorizontalAlignment="Left" Height="23" Margin="819,103,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" Controls:TextBoxHelper.Watermark="容器Loottable种子"/>
